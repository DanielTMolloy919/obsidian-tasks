filters:
  or:
    - "!tags.isEmpty()"
formulas:
  ErrorsAreTagsValuesNeedingFixing: note.tags.join(" ")
views:
  - type: table
    name: Table
    order:
      - file.name
      - formula.ErrorsAreTagsValuesNeedingFixing
      - tags
      - file.path
    sort:
      - column: file.path
        direction: ASC
    columnSize:
      formula.ErrorsAreTagsValuesNeedingFixing: 375
      note.tags: 395
