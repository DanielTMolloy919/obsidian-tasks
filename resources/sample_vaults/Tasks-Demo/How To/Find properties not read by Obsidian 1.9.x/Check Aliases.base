filters:
  or:
    - "!alias.isEmpty()"
    - "!aliases.isEmpty()"
formulas:
  AliasesWithSpaceJoin: note.aliases.join(" ")
  AliasesAsList: list(note.aliases)
  AliasesToString: note.aliases.toString()
views:
  - type: table
    name: Table
    order:
      - file.name
      - aliases
      - note.ALIAS,
      - formula.AliasesWithSpaceJoin
      - formula.AliasesAsList
      - formula.AliasesToString
      - file.path
    sort:
      - column: file.path
        direction: ASC
